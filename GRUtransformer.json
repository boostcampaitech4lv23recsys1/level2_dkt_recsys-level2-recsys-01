{
    "user": "wonjun",
    "project": "GRUtransformer",
    "entity": "dkt-dinosaur",
  
    "cat_cols": [
      "KnowledgeTag",
      "question_number",
      "test_cat",
      "test_id",
      "test_month",
      "test_day",
      "test_hour",
      "week_num"
    ],
    "num_cols": [
      "elapsed_time",
      "time_question_median",
      "question_numslen",
      "tag_acc",
      "month_acc",
      "hour_acc",
      "exp_test",
      "exp_tag",
      "exp_question",
      "ans_cumsum"
    ],

    "preprocess": {
      "data_dir": "./data",
      "data_ver": "v2.1",
      "fe_elapsed_time": [200, "mean"],
      "use_k_fold": true,
      "num_fold": 5
    },
  
    "dataset": {
      "type": "BaseDataset",
      "max_seq_len": 200
    },
  
    "data_loader": {
      "type": "사용할 DataLoader",
      "args": {
        "batch_size": 64,
        "num_workers": 4,
        "collate_fn": "collate_fn"
      }
    },
  
    "arch": {
      "type": "GRUtransformer",
      "args": {
        "hidden_dim" : 128,
        "n_layers" : 2,
        "dim_model": 128,
        "dim_ffn": 64,
        "embedding_dim": 64,
        "num_heads": 2,
        "n_layers_transformer": 1,
        "n_layers_GRU": 1,
        "dropout_rate": 0.1
      }
    },
  
    "loss": "bce",
  
    "optimizer": {
      "type": "adam",
      "args": {
        "lr": 0.001,
        "weight_decay": 0,
        "amsgrad": false
      }
    },
  
    "trainer": {
      "type": "사용할 Trainer",
      "epochs": 30,
      "metric": ["aucroc", "accuracy"],
      "seed": 42,
      "save_dir": "/opt/ml/model",
      "submission_dir": "/opt/ml/submission"
    }
  }