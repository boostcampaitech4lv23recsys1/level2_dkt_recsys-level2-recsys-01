{
    "user": "wonjun",
    "project": "GRUtransformer",
    "entity": "dkt-dinosaur",
  
    "cat_cols": [
        "assessmentItemID",
        "test_id",
        "question_number",
        "KnowledgeTag"
      ],
    "num_cols": [
        "elapsed_time",
        "ans_cumavg",
        "ans_cumsum",
        "time_question_median",
        "tag_acc",
        "elapsed_time_mean",
        "elapsed_time_median",
        "KnowledgeTag_et_std",
        "KnowledgeTag_et_mean",
        "test_acc",
        "test_day",
        "test_hour",
        "hour_acc",
        "exp_tag",
        "KnowledgeTag_aC_mean"
      ],

    "preprocess": {
      "data_dir": "./data",
      "data_ver": "v2.2",
      "fe_elapsed_time": [200, "mean"],
      "use_k_fold": true,
      "num_fold": 5
    },
  
    "dataset": {
      "type": "BaseDataset",
      "max_seq_len": 150
    },
  
    "data_loader": {
      "type": "사용할 DataLoader",
      "args": {
        "batch_size": 64,
        "num_workers": 4,
        "collate_fn": "collate_fn"
      }
    },
  
    "arch": {
      "type": "GRUtransformer",
      "args": {
        "hidden_dim" : 128,
        "n_layers" : 2,
        "dim_model": 128,
        "dim_ffn": 64,
        "embedding_dim": 64,
        "num_heads": 2,
        "n_layers_transformer": 1,
        "n_layers_GRU": 2,
        "dropout_rate": 0.1
      }
    },
  
    "loss": "bce",
  
    "optimizer": {
      "type": "adam",
      "args": {
        "lr": 0.001,
        "weight_decay": 0,
        "amsgrad": false
      }
    },
  
    "trainer": {
      "type": "사용할 Trainer",
      "epochs": 100,
      "metric": ["aucroc", "accuracy"],
      "seed": 42,
      "save_dir": "/opt/ml/model",
      "submission_dir": "/opt/ml/submission"
    }
}