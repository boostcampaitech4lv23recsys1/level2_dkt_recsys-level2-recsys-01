{
    "project" : "LSTM",
    "entity" : "dkt-dinosaur",
    "user": "eunhye",
    
    "cat_cols": [
        "assessmentItemID",
        "test_id",
        "question_number",
        "KnowledgeTag"
    ],
    "num_cols": [
        "elapsed_time",
        "ans_cumavg",
        "ans_cumsum",
        "time_question_median",
        "tag_acc",
        "elapsed_time_mean",
        "elapsed_time_median",
        "KnowledgeTag_et_std",
        "KnowledgeTag_et_mean",
        "test_acc",
        "test_day",
        "test_hour",
        "hour_acc",
        "exp_tag",
        "KnowledgeTag_aC_mean"
    ],
    "one_embedding": false,
  
  
    "preprocess": {
          "data_dir" : "./data",
          "data_ver" : "v2.2",
          "fe_elapsed_time": [200, "mean"],
          "use_k_fold": true,
          "num_fold" : 5,
          "data_augmentation": false
    },
  
    "dataset" : {
          "type": "BaseDataset",
          "max_seq_len": 500
    },
  
    "data_loader" : {
        "type": "사용할 DataLoader",
        "args": {
            "batch_size" : 128,
            "num_workers" : 8,
            "collate_fn" : "collate_fn"
        }
    },
  
    "arch" : {
        "type": "LSTM",
        "args": {
            "hidden_dim" : 128,
            "embedding_dim" : 64,
            "n_layers" : 2,
            "dropout_rate" : 0.1
        }
    },
  
    "loss" : "bce",
  
    "optimizer" : {
        "type": "adam",
        "args":{
            "lr": 0.001,
            "weight_decay": 0.1,
            "amsgrad": false
        }
    },
  
    "trainer" : {
        "epochs": 25,
        "metric": ["aucroc", "accuracy"],
        "seed" : 42,
        "save_dir": "./saved_model",
        "submission_dir": "./submission"
    }
}