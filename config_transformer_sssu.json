{
    "project": "transformer",
    "entity": "dkt-dinosaur",
    "user": "sssu><",
  
    "cat_cols": [
      "KnowledgeTag",
      "question_number",
      "assessmentItemID"
    ],
    "num_cols": ["elapsed_time_mean",
    "time_question_median",
    "tag_acc",
    "test_acc",
    "KnowledgeTag_et_mean"
  ],
  
    "preprocess": {
      "data_dir": "./data",
      "data_ver": "v2.2",
      "fe_elapsed_time": [200, "mean"],
      "use_k_fold": true,
      "num_fold": 5
    },
  
    "dataset": {
      "type": "BaseDataset",
      "max_seq_len": 128
    },
  
    "data_loader": {
      "type": "사용할 DataLoader",
      "args": {
        "batch_size": 16,
        "num_workers": 4,
        "collate_fn": "collate_fn"
      }
    },
  
    "arch": {
      "type": "Transformer",
      "args": {
        "dim_model": 128,
        "dim_ffn": 64,
        "embedding_dim": 64,
        "num_heads": 2,
        "n_layers": 1,
        "dropout_rate": 0.2
      }
    },
  
    "loss": "bce",
  
    "optimizer": {
      "type": "adam",
      "args": {
        "lr": 0.001,
        "weight_decay": 0,
        "amsgrad": false
      }
    },
  
    "trainer": {
      "type": "사용할 Trainer",
      "epochs": 20,
      "metric": ["aucroc", "accuracy"],
      "seed": 42,
      "save_dir": "/opt/ml/model",
      "submission_dir": "/opt/ml/submission"
    }
  }
  