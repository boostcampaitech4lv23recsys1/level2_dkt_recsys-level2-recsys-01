{
    "project": "transformer",
    "entity": "dkt-dinosaur",
    "user": "sssu><",
  
    "cat_cols": [
      "KnowledgeTag",
      "question_number",
      "test_cat",
      "test_id",
      "test_month",
      "test_hour",
      "week_num"
    ],
    "num_cols": ["elapsed_time",
    "time_question_median",
    "question_numslen",
    "tag_acc",
    "month_acc",
    "hour_acc",
    "exp_test",
    "exp_tag",
    "exp_question",
    "ans_cumsum",
    "continuous_test",
        "test_cat_aC_mean", "question_number_aC_mean", "KnowledgeTag_aC_mean",
        "week_num_aC_mean", "test_cat_et_mean", "question_number_et_mean",
        "KnowledgeTag_et_mean", "week_num_et_mean", "test_cat_et_std",
        "question_number_et_std", "KnowledgeTag_et_std", "week_num_et_std"
    ],
  
    "preprocess": {
      "data_dir": "./data",
      "data_ver": "v2.2",
      "fe_elapsed_time": [200, "mean"],
      "use_k_fold": true,
      "num_fold": 5
    },
  
    "dataset": {
      "type": "BaseDataset",
      "max_seq_len": 128
    },
  
    "data_loader": {
      "type": "사용할 DataLoader",
      "args": {
        "batch_size": 16,
        "num_workers": 4,
        "collate_fn": "collate_fn"
      }
    },
  
    "arch": {
      "type": "Transformer",
      "args": {
        "dim_model": 128,
        "dim_ffn": 64,
        "embedding_dim": 64,
        "num_heads": 2,
        "n_layers": 1,
        "dropout_rate": 0.2
      }
    },
  
    "loss": "bce",
  
    "optimizer": {
      "type": "adam",
      "args": {
        "lr": 0.001,
        "weight_decay": 0,
        "amsgrad": false
      }
    },
  
    "trainer": {
      "type": "사용할 Trainer",
      "epochs": 10,
      "metric": ["aucroc", "accuracy"],
      "seed": 42,
      "save_dir": "/opt/ml/model",
      "submission_dir": "/opt/ml/submission"
    }
  }
  